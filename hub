    -------------------------- Functions
    function autoRemote()
        while getgenv().AutoRemote == true do
            for i, v in pairs(myfac:GetDescendants()) do
                if v.Name == "DropRate" then
                    if v.Value == "Manual" then
                        game:GetService("ReplicatedStorage"):WaitForChild("RemoteDrop"):FireServer()
                    end
                end
